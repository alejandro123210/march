target 'march' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for march
pod 'Firebase/Core'
pod 'Firebase/Database'

  target 'marchTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'marchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
